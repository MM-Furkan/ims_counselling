<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_on_Referred_by_Employee</fullName>
    <active>true</active>
    <errorConditionFormula>AND(TEXT(Referred_By__c)=&apos;Employee&apos;,ISBLANK(Employee_Referral_Name__c))</errorConditionFormula>
    <errorDisplayField>Employee_Referral_Name__c</errorDisplayField>
    <errorMessage>Enter Employee Name.</errorMessage>
</ValidationRule>
